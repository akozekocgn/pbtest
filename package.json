{
  "name": "hehe",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "serve": "NODE_ENV=production babel-node index.js",
    "server": "NODE_ENV=development nodemon --inspect --exec babel-node index.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build-client": "npm run-script build --prefix client",
    "build-api": "babel api --out-dir dist/api --source-maps inline; babel index.js -d dist; babel server.js -d dist;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akozekocgn/pbtest.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akozekocgn/pbtest/issues"
  },
  "homepage": "https://github.com/akozekocgn/pbtest#readme",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "cookie-parser": "^1.4.4",
    "create-react-app": "^3.0.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "helmet": "^3.20.0",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "prisma-client-lib": "^1.34.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5"
  }
}
